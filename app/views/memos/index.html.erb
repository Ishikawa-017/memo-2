<h3>Memo Top</h3>
<a href="/new" button type="button" class="btn btn-default", id:"new button">New Memo</a>
<h3>Memo Title</h3>
<% @memos.each do |memo| %>
    <div class="panel panel-default">
        <!--<a href="/:id" class:"glyphicon glyphicon-circ.le-arrow-right"></a>-->
      <div class="panel-body">
        <h4><%= memo.title %></h4>
           <div class="button">
                <a href="/browse/<%=memo.id%>"><span class="glyphicon glyphicon-search"></span></a>
                <a href="/memos/<%=memo.id%>/edit"><span class="glyphicon glyphicon-edit"></span></a>
                <a href="/memos/<%=memo.id%>" data-method="delete"><span class="glyphicon glyphicon-trash"></span></a>
            </div>
      </div>
    </div>
<% end %>
