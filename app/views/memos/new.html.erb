<h3>New Memo</h3>
<%= form_tag("/create") do %>
      <div class="form-group">
        <%= label_tag "form-title", "Title" %>
        <%= text_field :memos, :title,  class:"form-control", id:"form-title", placeholder: "Title"%>
      </div>
      <div class="form-group">
        <%= label_tag "form-content", "Content" %>
        <%= text_field :memos, :body, class:"form-control", id:"form-content", placeholder: "Content" %>
      </div>
      
        <%= submit_tag "Keep", class:"btn btn-primary"%>

<% end %>